{
	"name": "odoo-develop",
	"dockerComposeFile": "docker-compose.yml",
	"service": "odoo",
	"workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
	"features": {},
	"customizations": {
		"vscode": {
			"extensions": [
				"Codeium.codeium",
				"ms-python.python"
			],
			"settings": {
				"terminal.integrated.defaultProfile.linux": "zsh",
				"terminal.integrated.profiles.linux": {
					"zsh": {
						"path": "zsh"
					}
				} 
			}
		}
	},
	"remoteUser": "vscode"
}
